&rpm_bus {
	rpm-regulator-ldoa18 {
		compatible = "qcom,rpm-smd-regulator-resource";
		qcom,resource-name = "ldoa";
		qcom,resource-id = <18>;
		qcom,regulator-type = <0>;
		status = "disabled";

		regulator-l18 {
			compatible = "qcom,rpm-smd-regulator";
			regulator-name = "pm660_l18";
			qcom,set = <3>;
			status = "disabled";
		};
	};

	rpm-regulator-ldoa18 {
		status = "okay";
		pm660_l18: regulator-l18 {
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			status = "okay";
		};
	};
};

&soc {
	led_flash0: qcom,camera-flash@0 {
		qcom,flash-source = <&pm660l_flash0>;
		qcom,torch-source = <&pm660l_torch0>;
		qcom,led-name = "led_flash0";
	};

	led_flash1: qcom,camera-flash@1 {
		qcom,flash-source = <&pm660l_flash1>;
		qcom,torch-source = <&pm660l_torch1>;
		qcom,led-name = "led_flash1";
	};
};

&tlmm {
		cam_sensor_front2_active: cam_sensor_front2_active {
			/* RESET */
			mux {
				pins = "gpio48", "gpio51";
				function = "gpio";
			};

			config {
				pins = "gpio48", "gpio51";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		cam_sensor_front2_suspend: cam_sensor_front2_suspend {
			/* RESET */
			mux {
				pins = "gpio48", "gpio51";
				function = "gpio";
			};

			config {
				pins = "gpio48", "gpio51";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		cam_sensor_rear_active: cam_sensor_rear_active {
			/* RESET */
			mux {
				pins = "gpio46";
				function = "gpio";
			};

			config {
				pins = "gpio46";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

		cam_sensor_rear_suspend: cam_sensor_rear_suspend {
			/* RESET */
			mux {
				pins = "gpio46";
				function = "gpio";
			};

			config {
				pins = "gpio46";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
			};
		};

	cam_sensor_cameraid: cam_sensor_cameraid {
		mux {
			pins = "gpio49";
			function = "gpio";
		};

		config {
			pins = "gpio49";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};
};

&cci {
	actuator0: qcom,actuator@0 {
		cam_vaf-supply = <&pm660l_l3>;
		cam_vafbk-supply = <&pm660l_l8>;
		qcom,cam-vreg-name = "cam_vaf", "cam_vafbk";
		qcom,cam-vreg-min-voltage = <2800000 2800000>;
		qcom,cam-vreg-max-voltage = <2800000 2800000>;
		qcom,cam-vreg-op-mode = <100000 100000>;
	};

	actuator1: qcom,actuator@1 {
		cam_vaf-supply = <&pm660l_l3>;
		cam_vafbk-supply = <&pm660l_l8>;
		qcom,cam-vreg-name = "cam_vaf", "cam_vafbk";
		qcom,cam-vreg-min-voltage = <2800000 2800000>;
		qcom,cam-vreg-max-voltage = <2800000 2800000>;
		qcom,cam-vreg-op-mode = <100000 100000>;
	};

	ois0: qcom,ois@0 {
		/delete-property/	gpios;
		/delete-property/	qcom,gpio-vaf;
		/delete-property/	qcom,gpio-req-tbl-num;
		/delete-property/	qcom,gpio-req-tbl-flags;
		/delete-property/	qcom,gpio-req-tbl-label;
		/delete-property/	pinctrl-names;
		/delete-property/	pinctrl-0;
		/delete-property/	pinctrl-1;
		/delete-property/	status;
		cell-index = <1>;
		qcom,cci-master = <1>;
		cam_vaf-supply = <&pm660l_l3>;
		cam_vafbk-supply = <&pm660l_l8>;
		qcom,cam-vreg-name = "cam_vaf", "cam_vafbk";
		qcom,cam-vreg-min-voltage = <2800000 2800000>;
		qcom,cam-vreg-max-voltage = <2800000 2800000>;
		qcom,cam-vreg-op-mode = <100000 100000>;
	};

	i2c_freq_100Khz: qcom,i2c_standard_mode {
		qcom,hw-scl-stretch-en = <1>;
	};

	i2c_freq_400Khz: qcom,i2c_fast_mode {
		qcom,hw-scl-stretch-en = <1>;
	};

	eeprom0: qcom,eeprom@0 {
		cam_vio-supply = <&pm660_l18>;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <10000>;
		gpios = <&tlmm 32 0>,
			<&tlmm 46 0>,
			<&tlmm 52 0>,
			<&tlmm 13 0>,
			<&tlmm 50 0>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK0",
					"CAM_RESET0",
					"CAM_VDIG",
					"CAM_VIO",
					"CAM_VANA";
		qcom,csiphy-sd-index = <0>;
		qcom,csid-sd-index = <0>;
		qcom,mount-angle = <90>;
		qcom,gpio-vdig = <2>;
		qcom,gpio-vio = <3>;
		qcom,gpio-vana = <4>;
		qcom,cam-power-seq-type = "sensor_gpio",
					"sensor_gpio",
					"sensor_vreg",
					"sensor_gpio",
					"sensor_clk",
					"sensor_gpio";
		qcom,cam-power-seq-val = "sensor_gpio_vana",
					"sensor_gpio_vio",
					"cam_vio",
					"sensor_gpio_vdig",
					"sensor_cam_mclk",
					"sensor_gpio_reset";
		qcom,cam-power-seq-cfg-val = <1 1 1 1 24000000 1>;
		qcom,cam-power-seq-delay = <1 1 1 10 1 25>;
	};

	eeprom1: qcom,eeprom@1 {
		cam_vio-supply = <&pm660_l18>;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <105000>;
		pinctrl-0 = <&cam_sensor_mclk3_active
				 &cam_sensor_front2_active>;
		pinctrl-1 = <&cam_sensor_mclk3_suspend
				 &cam_sensor_front2_suspend>;
		gpios = <&tlmm 35 0>,
			<&tlmm 42 0>,
			<&tlmm 12 0>,
			<&tlmm 51 0>,
			<&tlmm 13 0>;
		qcom,gpio-vdig = <2>;
		qcom,gpio-vana = <3>;
		qcom,gpio-vio = <4>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK",
				"CAM_RESET",
				"CAM_VDIG",
				"CAM_VANA",
				"CAM_VIO";
		qcom,cam-power-seq-type = "sensor_clk",
					"sensor_gpio",
					"sensor_vreg",
					"sensor_gpio",
					"sensor_gpio",
					"sensor_gpio";
		qcom,cam-power-seq-val = "sensor_cam_mclk",
					"sensor_gpio_vana",
					"cam_vio",
					"sensor_gpio_vio",
					"sensor_gpio_vdig",
					"sensor_gpio_reset";
		qcom,cam-power-seq-cfg-val = <24000000 1 1 1 1 1>;
		qcom,cam-power-seq-delay = <1 1 1 1 1 10>;
		clocks = <&clock_mmss MCLK3_CLK_SRC>,
			<&clock_mmss MMSS_CAMSS_MCLK3_CLK>;
	};

	eeprom2: qcom,eeprom@2 {
		qcom,csiphy-sd-index = <2>;
		qcom,csid-sd-index = <2>;
		qcom,mount-angle = <0>;
		cam_vio-supply = <&pm660_l18>;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <105000>;
		gpios = <&tlmm 33 0>,
			<&tlmm 47 0>,
			<&tlmm 52 0>,
			<&tlmm 51 0>,
			<&tlmm 13 0>;
		qcom,gpio-vdig = <2>;
		qcom,gpio-vana = <3>;
		qcom,gpio-vio = <4>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK1",
					"CAM_RESET1",
					"CAM_VDIG1",
					"CAM_VANA1",
					"CAM_VIO1";
		qcom,cam-power-seq-type = "sensor_vreg",
					"sensor_gpio",
					"sensor_gpio",
					"sensor_gpio",
					"sensor_clk",
					"sensor_gpio";
		qcom,cam-power-seq-val = "cam_vio",
					"sensor_gpio_vio",
					"sensor_gpio_vana",
					"sensor_gpio_vdig",
					"sensor_cam_mclk",
					"sensor_gpio_reset";
		qcom,cam-power-seq-cfg-val = <1 1 1 1 24000000 1>;
		qcom,cam-power-seq-delay = <5 5 5 5 5 5>;
	};

	qcom,camera@0 {
		qcom,mount-angle = <90>;
		/delete-property/	qcom,ois-src;
		cam_vio-supply = <&pm660_l18>;
		/delete-property/	cam_vana-supply;
		/delete-property/	cam_vdig-supply;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <10000>;
		gpios = <&tlmm 32 0>,
			<&tlmm 46 0>,
			<&tlmm 52 0>,
			<&tlmm 13 0>,
			<&tlmm 50 0>;
		qcom,gpio-reset = <1>;
		qcom,gpio-vdig = <2>;
		qcom,gpio-vio = <3>;
		qcom,gpio-vana = <4>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK0",
					"CAM_RESET0",
					"CAM_VDIG",
					"CAM_VIO",
					"CAM_VANA";
	};

	qcom,camera@1 {
		qcom,led-flash-src = <&led_flash1>;
		qcom,ois-src = <&ois0>;
		cam_vio-supply = <&pm660_l18>;
		/delete-property/	cam_vana-supply;
		/delete-property/	cam_vdig-supply;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <105000>;
		pinctrl-0 = <&cam_sensor_mclk3_active
				 &cam_sensor_front2_active>;
		pinctrl-1 = <&cam_sensor_mclk3_suspend
				 &cam_sensor_front2_suspend>;
		gpios = <&tlmm 35 0>,
			<&tlmm 42 0>,
			<&tlmm 12 0>,
			<&tlmm 51 0>,
			<&tlmm 13 0>;
		qcom,gpio-vdig = <2>;
		qcom,gpio-vana = <3>;
		qcom,gpio-vio = <4>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK",
				"CAM_RESET",
				"CAM_VDIG",
				"CAM_VANA",
				"CAM_VIO";
		qcom,sensor-position = <1>;
		clocks = <&clock_mmss MCLK3_CLK_SRC>,
			<&clock_mmss MMSS_CAMSS_MCLK3_CLK>;
	};

	qcom,camera@2 {
		cam_vio-supply = <&pm660_l18>;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1950000>;
		qcom,cam-vreg-op-mode = <105000>;
		pinctrl-0 = <&cam_sensor_mclk1_active
				 &cam_sensor_front_active
				 &cam_sensor_cameraid>;
		gpios = <&tlmm 33 0>,
			<&tlmm 47 0>,
			<&tlmm 52 0>,
			<&tlmm 51 0>,
			<&tlmm 13 0>,
			<&tlmm 49 0>;
		qcom,gpio-vdig = <2>;
		qcom,gpio-vana = <3>;
		qcom,gpio-vio = <4>;
		qcom,gpio-standby = <5>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4 5>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0 1>;
		qcom,gpio-req-tbl-label = "CAMIF_MCLK1",
					"CAM_RESET1",
					"CAM_VDIG1",
					"CAM_VANA1",
					"CAM_VIO1",
					"CAM_ID";
	};
	/delete-node/ qcom,camera@3;
};
